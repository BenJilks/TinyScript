include "TinyLib/TinyLib.tiny"

class ReadOnlyList
{
    items : List
    
    func ReadOnlyList(items : List)
    {
        self.items = items
    }
    
    func for_item(callback)
    {
        for item in items
            call_func(callback, item)
    }
}

func print_item(item)
    println(item)

func main()
{
    list : List = ["Very", "Important", "Stuff"]
    read_only_list : ReadOnlyList = ReadOnlyList(list)
    read_only_list.for_item(print_item)
}
